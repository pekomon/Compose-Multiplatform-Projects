CREATE TABLE RunHistory(
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  difficulty TEXT NOT NULL,
  elapsedMillis INTEGER NOT NULL,
  finishedAt INTEGER NOT NULL
);

insert:
INSERT INTO RunHistory(difficulty, elapsedMillis, finishedAt)
VALUES (?, ?, ?);

selectAllByDifficulty:
SELECT * FROM RunHistory
WHERE difficulty = ?
ORDER BY elapsedMillis ASC, finishedAt ASC
LIMIT 10;
